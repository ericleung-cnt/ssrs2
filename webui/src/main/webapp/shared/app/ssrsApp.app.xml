<Application>
    <rpcBindings>
        <ServerObject ID="securityDMI" lookupStyle="spring" bean="securityDMI">
            <visibleMethods>
                <method name="checkLogin"/>
                <method name="getPIA"/>
                <method name="login"/>
                <method name="logout"/>
            </visibleMethods>
        </ServerObject>
        <ServerObject ID="reportDMI" lookupStyle="spring" bean="reportDMI">
            <visibleMethods>
                <method name="generate"/>
            </visibleMethods>
        </ServerObject>
    </rpcBindings>
</Application>