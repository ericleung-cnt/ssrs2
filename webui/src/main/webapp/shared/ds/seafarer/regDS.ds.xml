<DataSource ID="regDS" serverType="generic" inheritsFrom="abstractPersistentEntityDS">
	<serverObject lookupStyle="spring" bean="regDMI" />
	<fields>
		<field name="seafarerId" 	type="text" 	title="Seafarer ID" required="true"/>
		<field name="seqNo" 		type="integer" 	title="Record No" 	/>
		<field name="regDate" 		type="date" displayFormat="dd/MM/yyyy" 	title="Record Date"/>
		<field name="regExpiry" 	type="date" displayFormat="dd/MM/yyyy" 	title="Expiry Date"/>
		<field name="regCancel" 	type="date" displayFormat="dd/MM/yyyy" 	title="Cancellation Date"/>
	</fields>
</DataSource>